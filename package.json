{
  "name": "qs-ext-jump-start",
  "description": "Template for Qlik Sense extension development with component system, template system, declarative rendering, Nebula CLI and Playwright testing.",
  "keywords": [
    "qlik",
    "qlik-sense",
    "extension",
    "nebula",
    "playwright",
    "template",
    "declarative-rendering",
    "component-system",
    "ui-templates"
  ],
  "version": "0.6.0",
  "license": "SEE LICENSE IN license.txt",
  "repository": {
    "type": "git",
    "url": "https://github.com/QlikSenseStudios/qs-ext-jump-start.git"
  },
  "bugs": {
    "url": "https://github.com/QlikSenseStudios/qs-ext-jump-start/issues"
  },
  "homepage": "https://github.com/QlikSenseStudios/qs-ext-jump-start#readme",
  "contributors": [
    {
      "name": "Qlik Professional Services"
    },
    {
      "name": "Karthik Burra",
      "email": "karthik.burra@qlik.com"
    },
    {
      "name": "Gabriel Wingard",
      "email": "gabriel.wingard@qlik.com"
    }
  ],
  "scripts": {
    "prepare": "husky",
    "lint": "eslint -c eslint.config.mjs src test",
    "lint-staged": "lint-staged",
    "build": "nebula build",
    "serve": "nebula serve",
    "test": "playwright test",
    "serve:report": "playwright show-report test/report",
    "package": "nebula sense --meta src/meta.json"
  },
  "engines": {
    "node": ">=20.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@nebula.js/cli": "^6.0.1",
    "@nebula.js/cli-build": "^6.0.1",
    "@nebula.js/cli-sense": "^6.0.1",
    "@nebula.js/cli-serve": "^6.0.1",
    "@playwright/test": "^1.55.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.37.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.4",
    "prettier": "^3.6.2"
  },
  "peerDependencies": {
    "@nebula.js/stardust": "^6.0.1"
  },
  "lint-staged": {
    "*.{js,jsx,mjs,cjs}": [
      "eslint -c eslint.config.mjs --fix"
    ]
  },
  "files": [
    "dist"
  ],
  "main": "dist/qs-ext-jump-start.js",
  "module": "dist/qs-ext-jump-start.esm.js"
}